{
	"$schema": "https://typedoc.org/schema.json",
	"out": "docs",
	"entryPoints": [
		"./src",
		"./packages/*"
	],
	"entryPointStrategy": "packages",
	"externalPattern": [
		"**/node_modules/**"
	],
	"excludeExternals": true,
	"excludePrivate": true,
	"excludeProtected": true,
	"readme": "./README.md",
	"media": "./media",
	"includes": "./mediaDocs",
	"logLevel": "Verbose",
	"plugin": ["typedoc-theme-yaf", "typedoc-plugin-versions"],
	"theme": "yaf",
	"versions": {
		"homeUrl": "http://localhost:3000/"
	}
}